{
  "version": 2,
  "rewrites": [
    {
      "source": "/webhook",
      "destination": "/api/index.js"
    },
    {
      "source": "/send-message",
      "destination": "/api/index.js"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}