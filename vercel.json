{
  "version": 2,
  "functions": {
    "server.js": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/webhook",
      "dest": "/server.js"
    },
    {
      "src": "/send-message",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/src/index.html" 
    }
  ]
}